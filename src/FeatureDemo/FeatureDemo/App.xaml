<Application
    x:Class="FeatureDemo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:common="using:FeatureDemo.Common">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                

                <ResourceDictionary Source="ms-appx:///View/Brushes.xaml" />
                <common:CommonStyles />
                <ResourceDictionary Source="ms-appx:///RibbonDemo/RibbonDemoCommonStyles.xaml" />
                <ResourceDictionary Source="Common/CodeViewControl/Themes/CodeViewControl.xaml" />
                <ResourceDictionary Source="ms-appx:///GridDemo/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            <x:String x:Key="AppName">FeatureDemo</x:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
